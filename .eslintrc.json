{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": [
        "*.test.js"
    ],
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "object-curly-newline": "off"
    }

}
